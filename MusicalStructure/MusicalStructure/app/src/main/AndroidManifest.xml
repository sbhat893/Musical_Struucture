<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.examples.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PlayingActivity"
            android:label="@string/first_text_main" />
        <activity
            android:name=".AboutActivity"
            android:label="@string/fifth_text_main" />
        <activity
            android:name=".SongActivity"
            android:label="@string/second_text_main" />
        <activity
            android:name=".ArtistActivity"
            android:label="@string/fourth_text_main" />
        <activity android:name=".PlaylistActivity" android:label="@string/third_text_main"/>
    </application>

</manifest>